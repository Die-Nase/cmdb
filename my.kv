#: import TreeViewLabel kivy.uix.treeview.TreeViewLabel
<TreeViewLabel>:
    on_touch_down:
        print(self.text)
        app.root.display_metadata(self.parent, self)

<DatabaseManager>:

    BoxLayout:
        id: left
        size_hint: .3, 1
        pos_hint: {"x":0, "top":1}
        orientation: 'vertical'

        TreeView:
            id: tv
            hide_root: True

        Button:
            size: (200,50)
            size_hint: (None, None)
            text: "add_node"
            on_press: root.say_hello()
    
    GridLayout:
        id: right
        size_hint: .7, 1
        pos_hint: {"x":.3, "top":1}
        cols: 2
        row_force_default: True
        row_default_height: 40
            